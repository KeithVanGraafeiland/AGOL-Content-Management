# engine = create_engine("postgresql+psycopg2://sde:gis@localhost:5433/
                       
import psycopg2
from psycopg2 import sql

# Establish a connection to the PostgreSQL server
conn = psycopg2.connect(
    dbname="postgres",  # Connect to the default database to create a new one
    user="postgres",
    password="postgres",
    host="localhost",
    port="5432"
)

# Create a cursor object
cur = conn.cursor()

# SQL command to create a new database
create_db_command = sql.SQL("CREATE DATABASE {dbname} OWNER {owner}").format(dbname=sql.Identifier('OCIS'),owner=sql.Identifier('SDE'))

# Execute the SQL command
cur.execute(create_db_command)

# Commit the transaction
conn.commit()

# Close the cursor and connection
cur.close()
conn.close()
